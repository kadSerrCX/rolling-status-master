name: TypoFix
on: [push]
jobs:
  typofix:
    runs-on: ubuntu-latest
    steps:
      - uses: actions/checkout@v3
      - name: TypoFix Markdown files
        run: |
          find . -name "*.md" -exec sed -i 's/teh/the/g' {} +
      - name: Commit changes
        run: |
          git config --global user.name "github-actions[bot]"
          git config --global user.email "41898282+github-actions[bot]@users.noreply.github.com"
          git add .
          git commit -m "TypoFix: Automated typo correction" || echo "No changes to commit"
          git pushname: Deploy ML Model
